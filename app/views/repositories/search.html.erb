<h1>Search Github Repositories</h1>
<%= form_tag '/search' do %>
  <%= label_tag :query %>
  <%= text_field_tag :query %>
  <%= submit_tag "Search Github!" %>
<% end %>

<% unless @repo.nil? %>
  <%= @repo['name'] %>
  <%= link_to @repo['html_url'], @repo['html_url'] %>
  <%= @repo['description'] %>
<% end %>
